package game.interaction;

public class Warrior {
	// Warrior 의 특성으로
	// 체력 마나 공격력 경험치 레벨을 넣어주세요
	
	public int hp;
	public int mp;
	public int atk;
	public int lv;
	public int exp;
	
	
	// 생성자를 생성해서 아무것도 입력받지 않고
	// 체력은 20 마나는 10 공격력은 4 레벨은 1 경험치는 0
	public Warrior() {
		this.hp=20;
		this.mp = 10;
		this.atk = 4;
		this.lv = 1;
		this.exp = 0;
	}

}
